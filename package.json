{
  "name": "polyglots-tools",
  "version": "0.1.1-9",
  "author": {
    "email": "allegretta92@hotmail.it",
    "name": "G. Allegretta"
  },
  "description": "An application that wants to simplify a bit the work for the WordPress Polyglots Team.",
  "homepage": "https://gitlab.com/gallegr-angular-projects/polyglots-tools",
  "repository": {
    "url": "git+https://gitlab.com/gallegr-angular-projects/polyglots-tools"
  },
  "bugs": {
    "url": "https://gitlab.com/gallegr-angular-projects/polyglots-tools/-/issues"
  },
  "license": "GPL-2.0-or-later",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:stage": "npm run minify:translations && ng build --configuration=stage && gzipper compress --verbose ./dist",
    "build:prod": "npm run minify:translations && ng build --prod && gzipper compress --verbose ./dist",
    "minify:translations": "npm run rename-translations && minify-json src/assets/i18n",
    "rename-translations": "renamer --find '/$/' --replace '-'$npm_package_version --path-element name 'src/assets/i18n/*'",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:html && npm run lint:style",
    "lint:ts": "ng lint",
    "lint:html": "npx linthtml 'src/app/**/*.html'",
    "lint:style": "npx stylelint 'src/**/*.(css|scss)'",
    "docs": "npm run docs:compodoc && npm run docs:bundle-info",
    "docs:compodoc": "compodoc",
    "docs:bundle-info": "webpack-bundle-analyzer dist/polyglots-tools/stats-es2015.json -m static -r docs/bundle-info/index.html",
    "perf:analyze": "npm run build && webpack-bundle-analyzer dist/polyglots-tools/stats-es2015.json",
    "perf:analyze-prod": "npm run build:prod && webpack-bundle-analyzer dist/polyglots-tools/stats-es2015.json",
    "e2e": "ng e2e",
    "version": "echo $npm_package_version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@ng-bootstrap/ng-bootstrap": "^6.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@compodoc/compodoc": "^1.1.11",
    "@linthtml/linthtml": "^0.5.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "gzipper": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "minify-json": "^1.0.0",
    "protractor": "~5.4.3",
    "renamer": "^2.0.1",
    "stylelint": "^13.7.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-no-empty": "^1.0.7",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
